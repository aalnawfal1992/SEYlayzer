<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seulayzer.ViewControl.UserManagmentControl">
   <children>
      <Pane layoutX="231.0" layoutY="72.0" prefHeight="271.0" prefWidth="330.0">
         <children>
            <TextField fx:id="txtUserName" layoutX="77.0" layoutY="40.0" prefHeight="25.0" prefWidth="171.0" promptText="Enter New User Name" style="-fx-background-radius: 10;" />
            <PasswordField fx:id="txtPassword" layoutX="77.0" layoutY="105.0" prefHeight="25.0" prefWidth="171.0" promptText="Enter User's Password" style="-fx-background-radius: 10;" />
            <PasswordField fx:id="txtConPassword" layoutX="77.0" layoutY="136.0" prefHeight="25.0" prefWidth="171.0" promptText="Confirm Password" style="-fx-background-radius: 10;" />
            <TextField fx:id="txtRealName" layoutX="77.0" layoutY="73.0" prefHeight="25.0" prefWidth="171.0" promptText="Enter User's Name" style="-fx-background-radius: 10;" />
            <Button fx:id="btnSubmit" layoutX="77.0" layoutY="171.0" mnemonicParsing="false" onAction="#adduser" prefHeight="25.0" prefWidth="61.0" style="-fx-background-radius: 10;" text="Submit">
               <font>
                  <Font name="Times New Roman Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnClear" layoutX="187.0" layoutY="171.0" mnemonicParsing="false" onAction="#cleartextdata" prefHeight="25.0" prefWidth="61.0" style="-fx-background-radius: 10;" text="Clear">
               <font>
                  <Font name="Times New Roman Bold" size="13.0" />
               </font>
            </Button>
            <Label layoutX="115.0" layoutY="14.0" prefHeight="17.0" prefWidth="95.0" text="Creat New User" textAlignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="lblPassMassage" contentDisplay="CENTER" layoutX="72.0" layoutY="203.0" prefHeight="62.0" prefWidth="187.0" textAlignment="CENTER" textFill="RED" wrapText="true">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="400.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnBack" mnemonicParsing="false" onAction="#privouspage" prefHeight="51.0" prefWidth="201.0" style="-fx-background-radius: 10;" text="Go Back To Pervious Page">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnClose" mnemonicParsing="false" onAction="#logoff" prefHeight="62.0" prefWidth="205.0" style="-fx-background-radius: 10;" text="Close Application">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
