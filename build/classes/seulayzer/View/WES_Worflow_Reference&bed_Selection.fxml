<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seulayzer.ViewControl.WES_Ref_CL">
   <children>
      <Pane layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Pane fx:id="btnHome" layoutY="226.0" prefHeight="174.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="txtBedpath" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="367.0" promptText="Path to bed file (.bed)" style="-fx-background-radius: 10;">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font></TextField>
                  <Button fx:id="btnBed" layoutX="383.0" layoutY="14.0" mnemonicParsing="false" onAction="#refBed" prefHeight="26.0" prefWidth="182.0" style="-fx-background-radius: 10;" text="Select bed (.bed)">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="nxtButton" layoutX="468.0" layoutY="119.0" mnemonicParsing="false" onAction="#toWESData" prefHeight="26.0" prefWidth="88.0" text="Next">
                     <font>
                        <Font name="Times New Roman Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="btnHome" layoutX="14.0" layoutY="119.0" mnemonicParsing="false" onAction="#goHome" prefHeight="28.0" prefWidth="101.0" text="Home Page">
                     <font>
                        <Font name="Times New Roman Bold" size="15.0" />
                     </font>
                  </Button>
                  <Label fx:id="lblerrorbed" layoutX="19.0" layoutY="54.0" prefHeight="26.0" prefWidth="357.0" textFill="#ee0a0a">
                     <font>
                        <Font name="Times New Roman Bold" size="15.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane layoutX="-3.0" prefHeight="196.0" prefWidth="607.0">
               <children>
                  <TextField fx:id="txtWESpath" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="367.0" promptText="Path to Reference Genome (.Fasta)" style="-fx-background-radius: 10;">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font></TextField>
                  <Button fx:id="btnWES" layoutX="381.0" layoutY="14.0" mnemonicParsing="false" onAction="#refSelect" prefHeight="26.0" prefWidth="182.0" style="-fx-background-radius: 10;" text="Select fReference (.Fasta)">
                     <font>
                        <Font name="Times New Roman Bold" size="13.0" />
                     </font>
                  </Button>
                  <Label fx:id="lblerror" layoutX="21.0" layoutY="52.0" prefHeight="26.0" prefWidth="357.0" textFill="#ee0a0a">
                     <font>
                        <Font name="Times New Roman Bold" size="15.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
         </children>
      </Pane>
   </children>
</AnchorPane>
